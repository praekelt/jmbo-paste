[buildout]
find-links=
    https://github.com/praekelt/panya/tarball/master#egg=panya
develop=\${buildout:directory}/src/$project/setup.py
extensions=lovely.buildouthttp
parts=
    django
eggs=
    panya
    $project

[django]
recipe = djangorecipe
version = 1.2.1
project = $project
projectegg = $project
settings = settings
eggs = \${buildout:eggs}
